{
  "manifest_version": 2,
  "name": "TradingView Without Ads",
  "version": "0.1",
  "description": "Removes TradingView Ads",
  "browser_action": {
    "default_icon": {
      "128": "icon-128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.tradingview.com/*"],
      "js": ["index.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon-128.png"
  }
}
